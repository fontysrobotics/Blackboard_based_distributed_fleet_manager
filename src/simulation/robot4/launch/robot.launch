<?xml version="1.0" ?>
<launch>


<param name="robot_description" command="$(find xacro)/xacro '$(find robot4)/urdf/robot_description_urdf.urdf'" />

 

    <node name="urdf_spawner4" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen" args="-urdf -x -1 -y 3 -z 0  -model robot4 -param robot_description"/>


   <node name="robot_state_publisher4" pkg="robot_state_publisher" type="robot_state_publisher" >

</node>


    <include file="$(find robot4)/launch/amcl.launch" />
    <include file="$(find robot4)/launch/move_base.launch" />

</launch>
