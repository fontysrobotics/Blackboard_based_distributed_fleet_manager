<launch>
  <param name="/use_sim_time" value="true" />

  <!-- start world -->
  <node name="gazebo" pkg="gazebo_ros" type="gazebo"
   args="$(find multiplebots)/worlds/small_office.world" respawn="false" output="screen" />

  <!-- include our robots -->
  <include file="$(find multiplebots)/launch/robots.launch"/>
</launch>
